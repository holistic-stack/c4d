{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "openscad",
      "camelcase": "Openscad",
      "title": "Openscad",
      "scope": "source.openscad",
      "file-types": [
        "scad"
      ],
      "injection-regex": "^openscad$",
      "class-name": "TreeSitterOpenscad"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Parse openscad code",
    "authors": [
      {
        "name": "Holistic Stack",
        "email": "holisticstack@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-openscad"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}