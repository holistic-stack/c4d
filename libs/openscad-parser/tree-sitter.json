{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "openscad_parser",
      "camelcase": "OpenscadParser",
      "title": "OpenscadParser",
      "scope": "source.openscad_parser",
      "file-types": [
        "openscad_parser"
      ],
      "injection-regex": "^openscad_parser$",
      "class-name": "TreeSitterOpenscadParser"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "OpenscadParser grammar for tree-sitter",
    "authors": [
      {
        "name": "Holistic Stack",
        "email": "holisticstack@gmail.com"
      }
    ],
    "links": {
      "repository": "https://github.com/tree-sitter/tree-sitter-openscad_parser"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}